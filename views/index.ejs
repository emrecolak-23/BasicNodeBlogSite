<html lang="eng">
  
  <%- include("./partials/head.ejs")  %> 

  <body>
    
    <%- include("./partials/nav.ejs") %> 

    <div class="blogs content">
      <h2>All Blogs</h2>
      <% if(blogs.length>0) { %> 
        <% blogs.forEach(blog => { %> 
          <h1 class="title"><%= blog.title %> </h1>
          <p class="snippet"><%= blog.snippet %> </p>
        <% }) %> 
      <% } else { %> 
        <p>There is no blogs to display</p>
      <% } %> 
    </div>

    <%- include("./partials/footer.ejs") %> 

  </body>

</html>